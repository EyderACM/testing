#include <stdio.h>

int main() {
    int numero,i,contador;

    printf("Ingresa un numero entero");
    scanf("%d",&numero);
    i=2;
    contador=0;
    if (numero<=1) {
        printf("Valor invalido");
    } else {
        while (i<=numero) {
            while (numero>1) {
                if (numero%i==0){
                    numero=numero/i;
                    contador=contador+i;
                } else {
                    i++;
                }
            }
            i++;
        }
    }
    printf("%d",contador+1);
    return 0;
}
